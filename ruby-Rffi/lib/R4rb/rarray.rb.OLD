module R4rb
  class Array < Array

  ## connect Array class to some RVector!!!
    def Array.initialize(rvect=".ary")
      @rb2R=R4rb::RVector.new(rvect)
      R4rb.init #if R4rb alive nothing is done!
    end

    def rvect
      @rb2R
    end

    def <<(val)
      self << val
      @rb2R < self
      return self
    end

    def <(val)
      if val.is_a? Array
        self += val
        @rb2R < self
        return self
      end 
    end

  end
end
 
